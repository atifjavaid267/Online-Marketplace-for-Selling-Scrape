
<div>
    <h2>New Address</h2>
</div>


<%= form_with(model: @address, local: true) do |form| %>
  <div class="form-group">
    <%= form.label :street1 %> <br>
    <%= form.text_field :street1, class: "form-control" %>
  </div>
  <br>

  <div class="form-group">
    <%= form.label :street2 %> <br>
    <%= form.text_field :street2, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :city %> <br>
    <%= form.text_field :city, class: "form-control" %>
  </div>
  <br>

  <div class="form-group">
    <%= form.label :state %> <br>
    <%= form.text_field :state, class: "form-control" %>
  </div>

  <br>

  <div class="form-group">
    <%= form.label :zip_code %> <br>
    <%= form.text_field :zip_code, class: "form-control" %>
  </div>
  <br>
  <div class="form-group">
    <%= form.label :latitude %> <br>
    <%= form.text_field :latitude, class: "form-control" %>
  </div>
  <br>
  <div class="form-group">
    <%= form.label :longitude %> <br>
    <%= form.text_field :longitude, class: "form-control" %>
  </div>
  <br>

  <div class="actions">
    <button>
        <%= form.submit "Submit", class: "btn btn-primary" %>
    </button>
  </div>

<%end%>
